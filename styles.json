                "composition": {
                  "type": "object",
                  "properties": {
                    "justification": {
                      "type": "string",
                      "enum": [
                        "AWAY_FROM_BINDING_SIDE",
                        "CENTER_ALIGN",
                        "CENTER_JUSTIFIED",
                        "FULLY_JUSTIFIED",
                        "LEFT_ALIGN",
                        "LEFT_JUSTIFIED",
                        "RIGHT_ALIGN",
                        "RIGHT_JUSTIFIED",
                        "TO_BINDING_SIDE"
                      ]
                    },
                    "singleWordJustification": {
                      "type": "string",
                      "enum": [
                        "CENTER_ALIGN",
                        "FULLY_JUSTIFIED",
                        "LEFT_ALIGN",
                        "RIGHT_ALIGN"
                      ]
                    },
                    "hyphenation": {
                      "anyOf": [
                        {
                          "type": "boolean",
                          "const": false,
                          "description": "Disables hyphenation."
                        },
                        {
                          "type": "object",
                          "properties": {
                            "hyphenWeight": {
                              "type": "number",
                              "description": "The relative desirability of better spacing or fewer hyphens. A lower value results in greater use of hyphens.",
                              "minimum": 0,
                              "maximum": 10
                            },
                            "hyphenateAcrossColumns": {
                              "type": "boolean",
                              "description": "If true, allows the last word in a text column to be hyphenated."
                            },
                            "hyphenateAfterFirst": {
                              "type": "number",
                              "description": "The mininum number of letters at the beginning of a word that can be broken by a hyphen.",
                              "minimum": 1,
                              "maximum": 15
                            },
                            "hyphenateBeforeLast": {
                              "type": "number",
                              "description": "The minimum number of letters at the end of a word that can be broken by a hyphen.",
                              "minimum": 1,
                              "maximum": 15
                            },
                            "hyphenateLadderLimit": {
                              "type": "number",
                              "description": "The maximum number of hyphens that can appear on consecutive lines. To specify unlimited consecutive lines, use zero.",
                              "minimum": 0,
                              "maximum": 25
                            },
                            "hyphenateLastWord": {
                              "type": "boolean",
                              "description": "If true, allows hyphenation in the last word in a paragraph. Note: Valid only when hyphenation is true."
                            }
                          }
                        }
                      ]
                    },
                    "balanceRaggedLines": {
                      "anyOf": [
                        { "type": "string" },
                        { "type": "boolean", "const": false }
                      ]
                    },
                    "firstLineIndent": { "$ref": "#/definitions/unit" },
                    "scaling": {
                      "type": "object",
                      "definition": "Glyph scaling, word spacing and letter spacing preferences.",
                      "properties": {
                        "horizontalScale": {
                          "type": "number",
                          "description": "The horizontal scaling applied to the glyphs."
                        },
                        "verticalScale": {
                          "type": "number",
                          "description": "The vertical scaling applied to the glyphs."
                        },
                        "glyphScaling": {
                          "type": "object",
                          "properties": {
                            "minimum": {
                              "type": "number",
                              "description": "The minimum width (%) of individual characters.",
                              "minimum": 50,
                              "maximum": 200
                            },
                            "desired": {
                              "type": "number",
                              "description": "The desired width (%) of individual characters.",
                              "minimum": 50,
                              "maximum": 200
                            },
                            "maximum": {
                              "type": "number",
                              "description": "The maximum width (%) of individual characters.",
                              "minimum": 50,
                              "maximum": 200
                            }
                          }
                        },
                        "letterSpacing": {
                          "type": "object",
                          "properties": {
                            "minimum": {
                              "type": "number",
