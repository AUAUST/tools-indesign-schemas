  "title": "Paragraphs and characters styles",
  "description": "Paragraphs and characters styles",
  "type": "object",
  "definitions": {
    "unit": {
      "anyOf": [
        { "type": "number" },
        {
          "type": "string",
          "pattern": "^-?[0-9]+(?:\\.[0-9]+)?(?:pt|mm|cm|in|i|)$"
        }
      ]
    }
  },
  "properties": {
    "paragraphs": {
      "type": "object",
      "description": "Object containing paragraph styles. The key's name becomes the paragraph style's name.",
      "patternProperties": {
        "^[a-zA-Z0-9_.-]+$": {
          "type": "object",
          "properties": {
            "isSnippet": {
              "type": "boolean",
              "description": "If set to true, it doesn't create a new paragraph style but you can use it with \"useSnippet\": [\"ids\"] in another paragraph style and the properties will be used."
            },
            "useSnippets": {
              "type": "array",
              "description": "Snippets to apply to the paragraph style. Can also be used inside a snippet paragraph style and it'll work as expected.\n\nIt uses no InDesign built-in inheritance but applies the properties directly.\n\nThe snippets used must be set BEFORE in the file.",
              "items": {
                "type": "string"
              }
            },
            "basedOn": {
              "type": "string",
              "description": "On which other paragraph style this one should be based. The input is the name of the target paragraph style. The target paragraph style must be set BEFORE in the file."
            },
            "text": {
              "type": "object",
              "description": "Configure text.",
              "properties": {
                "fontFamily": { "type": "string" },
                "fontStyle": { "type": "string" },
                "fontSize": { "$ref": "#/definitions/unit" },
                "variable": {
                  "anyOf": [
                    { "type": "boolean", "enum": [false] },
                    {
                      "type": "object",
                      "patternProperties": {
                        "^[a-zA-Z0-9_.-]+$": {
                          "not": "object"
                        }
                      },
                      "properties": {
                        "Weight": { "type": "number" },
                        "Width": { "type": "number" },
                        "Italic": { "type": "number" }
                      }
                    }
                  ]
                },
                "textTransform": {
                  "type": "string",
                  "enum": [
                    "SMALL_CAPS",
                    "NORMAL",
                    "CAP_TO_SMALL_CAP",
                    "ALL_CAPS"
                  ]
                },
                "characterAlignment": {
                  "type": "string",
                  "enum": [
                    "ALIGN_BASELINE",
                    "ALIGN_EM_BOTTOM",
                    "ALIGN_EM_CENTER",
                    "ALIGN_EM_TOP",
                    "ALIGN_ICF_BOTTOM",
                    "ALIGN_ICF_TOP"
                  ]
                },
                "characterDirection:": {
                  "type": "string",
                  "enum": [
                    "DEFAULT_DIRECTION",
                    "LEFT_TO_RIGHT_DIRECTION",
                    "RIGHT_TO_LEFT_DIRECTION"
                  ]
                },
                "characterRotation": {
                  "type": "number",
                  "description": "A value in degrees in which to rotate characters. Applied individually for each glyph and unclock-wise."
                },
                "numbers": {
                  "type": "object",
                  "properties": {}
                },
                "baseLineShift": { "$ref": "#/definitions/unit" },
                "dropCap": {
                  "type": "object",
                  "properties": {
                    "characters": {
                      "type": "number",
                      "description": "The number of characters to drop cap.",
                      "minimum": 0,
                      "maximum": 150
                    },
                    "lines": {
                      "type": "number",
                      "description": "The number of lines to drop cap.",
                      "minimum": 0,
                      "maximum": 25
                    },
                    "style": {
                      "type": "string"
                    },
                    "detail": {
                      "type": "number",
                      "description": "Drop cap's details. Align to left side: 1. Adjust descenders: 2. Enable both: 3. Disable both: 0.",
                      "minimum": 0,
                      "maximum": 3
                    }
                  }
                },
                "colors": {
                  "type": "object",
                  "properties": {
                    "fill": {
                      "type": "object",
                      "properties": {
                        "color": {
                          "type": "string"
                        },
                        "opacity": {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 100
                        }
                      }
                    }
                  }
